<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.arwinata.am.skripsi.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_voucher"
        android:name="com.arwinata.am.skripsi.ui.voucher.DashboardFragment"
        android:label="@string/title_voucher"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_mission"
        android:name="com.arwinata.am.skripsi.ui.mission.NotificationsFragment"
        android:label="@string/title_mission"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_profil"
        android:name="com.arwinata.am.skripsi.ui.profil.ProfilFragment"
        android:label="@string/title_mission"
        tools:layout="@layout/fragment_profil" />
</navigation>
